import java.util.*;
class R192224203{
    public static void main(String args[]){
        int n,sum=0;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        for(int i=1;i<n;i++) {
            if(n%i==0) {
                sum=sum+i;
            }
        }
        if(sum==n) {
            System.out.println("Perfect number");
        }
        else{
            System.out.println("Not a perfect number");
        }
    }
}
