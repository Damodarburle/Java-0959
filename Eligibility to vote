import java.util.*;
class R192224203{
    public static void main(String args[]){
        int age;
        Scanner s=new Scanner(System.in);
        age=s.nextInt();
        if(age>18) {
            System.out.println("Eligible to vote");
        }
        else{
            System.out.println(18-age+" "+" years are required for eligibility to vote");
        }
    }
}
